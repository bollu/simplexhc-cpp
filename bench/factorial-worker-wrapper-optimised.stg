# RUN: %loadSimplexhc %s  --jit | FileCheck %s
# CHECK: 120


binding factorial = \(a : PrimInt) -> PrimInt {
    case a () of
        0 -> 1;
        x -> case primSubtract (x 1) of
                xmin1 -> case primSubtract (xmin1 100) of
                                factxmin1 -> primMult (x factxmin1);;;
};


binding main = \() -> Void { 
    case  factorial (20) of
            x -> printInt (x);
};
